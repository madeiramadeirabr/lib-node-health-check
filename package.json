{
  "name": "lib-node-health-check",
  "version": "1.0.0",
  "description": "No description available",
  "main": "dist/presentation/lib/index.js",
  "scripts": {
    "test": "set ENVIRONMENT_NAME=test && jest",
    "coverage": "set ENVIRONMENT_NAME=test && jest --coverage",
    "prepare": "husky install",
    "build": "tsc",
    "postinstall": "npm run build"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "compiler": "ttypescript"
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "config/*",
      "test/fixture/*",
      "container.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/../test/setup/jest-ts-auto-mock-config.ts"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.109",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "aws-sdk": "^2.1368.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-ts-auto-mock": "^2.1.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "serverless": "^3.25.1",
    "serverless-newrelic-lambda-layers": "^3.4.0",
    "serverless-offline": "^12.0.2",
    "serverless-plugin-resource-tagging": "^1.2.0",
    "serverless-tag-cloud-watch-logs": "^1.0.0",
    "serverless-webpack": "^5.11.0",
    "ts-auto-mock": "3.6.4",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.2"
  }
}
